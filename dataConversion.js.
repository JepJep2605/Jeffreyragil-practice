

let price = "200";


let priceUsingParseInt = parseInt(price);
console.log(priceUsingParseInt); 

let priceUsingNumber = Number(price);
console.log(priceUsingNumber);

let priceUsingPlusOperator = +price;
console.log(priceUsingPlusOperator);



let distance = 150;


let distanceUsingToString = distance.toString();
console.log(distanceUsingToString);

let distanceUsingString = String(distance);
console.log(distanceUsingString);


let weight = "72.5";


let weightUsingParseFloat = parseFloat(weight);
console.log(weightUsingParseFloat);